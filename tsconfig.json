{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "navigation/*": ["src/navigation/*"],
      "components/*": ["src/components/*"],
      "screens/*": ["src/screens/*"],
      "stores/*": ["src/stores/*"],
      "styles/*": ["src/styles/*"],
      "utils": ["src/utils/*"],
      "types": ["src/types/*"],
      "src/*": ["src/*"]
    },
    "strict": true
  },
  "typeRoots": ["./types"]
}
